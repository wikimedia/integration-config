# EditorConfig is awesome: https://EditorConfig.org

# top-most EditorConfig file
root = true

[*]
insert_final_newline = true
indent_size = 4
indent_style = space

# Cause of left-padding spaces being used
[zuul-migrate]
indent_size = unset

[{*.groovy,*.xml}]
indent_size = 2

[LICENSE]
indent_size = unset

[KEYS]
indent_size = unset

[*.gpg]
# They are binary files
indent_size = unset
insert_final_newline = false
